*
  margin 0

html, body
  height: 100%;

.wrapper
  min-height: 100%
  height auto !important
  height 100%
  margin 53px auto -200px auto

.footer, .push
  height 200px

.footer
  line-height 11px
  font-size 9px
  padding 4px
  text-align center

.hero-unit
  padding 20px 60px

.title
  font-size 64px
  line-height 70px
  padding 6px 0px 6px 24px

.list-container
  padding 20px
  background-color #fff
  border 1px solid #bbb
  border-radius 4px

.login-bar
  margin-right 20px
  margin-top 8px
  font-size 12px


.nav li a i
  margin-right 6px

.subnav
  background-color #ddd
  border-bottom 2px solid #aaa
  width 100%
  position fixed
  height 36px
  margin-top -2px
  padding 2px 10px
  box-shadow: 0px 1px 14px #999;

.push-top
  padding-top 36px

.w32px
  width 32px

.graph-canvas
  background-image url("/img/cream_pixels.png")
  border 1px solid #ccc
  border-radius 4px

.fade-hide-setup, .fade-show-setup
  -webkit-transition all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.75s
  -moz-transition all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.75s
  -o-transition all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.75s
  transition all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.75s

.fade-hide-setup
  opacity 1

.fade-hide-setup.fade-hide-start
  opacity 0

.fade-show-setup
  opacity 0

.fade-show-setup.fade-show-start
  opacity 1

.panel-big
  min-height 800px
  padding 10px

.panel-big
  min-height 600px
  padding 10px

.panel-small
  min-height 380px
  padding 10px

.panel-white
  background-color #fff


.panel-green
  background-color #669966
  border-bottom 4px solid #88AA88
  box-shadow 0px 1px 14px #336633

.panel-about
  background-image url("/img/yggdrasill_trans350px.png")
  background-repeat no-repeat

.bigfooter
  background-color: rgb(44, 62, 80);
  background-image: -webkit-linear-gradient(top, rgb(44, 62, 80), rgb(44, 62, 80));